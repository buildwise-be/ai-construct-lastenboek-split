# AI Construct PDF Opdeler - Environment Configuration
# Copy this file to .env and update with your values

# Google Cloud Configuration (Required for AI processing)
GOOGLE_CLOUD_PROJECT_ID=your-project-id-here

# Optional: Vertex AI Configuration
VERTEX_AI_REGION=europe-west1
VERTEX_AI_MODEL=gemini-1.5-flash-001

# Optional: Application Settings
DEFAULT_OUTPUT_DIR=output
LOG_LEVEL=INFO
PROCESSING_TIMEOUT=300

# Optional: PDF Processing Settings
MAX_CONCURRENT_PROCESSES=3
BATCH_SIZE=10

# Optional: UI Settings
WINDOW_WIDTH=1000
WINDOW_HEIGHT=700
THEME=light